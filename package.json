{
  "name": "aegyris",
  "version": "3.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:test": "ng serve --configuration=test",
    "start:uat": "ng serve --configuration=uat",
    "start:e2e": "ng serve --configuration=e2e",
    "build": "ng build --aot",
    "build:test": "npm run build -- --configuration=test",
    "build:e2e": "npm run build -- --configuration=e2e",
    "build:prod": "npm run build -- --configuration=production",
    "build:ingenBio": "npm run build -- --configuration=ingenBio",
    "build:uat": "npm run build -- --configuration=uat",
    "build:nbrlab-prod": "npm run build -- --configuration=nbrlab-prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "analyze-bundle": "source-map-explorer dist/**/*.js",
    "teste2e": "npm run e2e-tests -- --config baseUrl=https://test-server.aegyris.ca ",
    "locale2e": "npm run start:e2e & wait-on http://localhost:4200 && npm run e2e-tests -- --config baseUrl=http://localhost:4200 ",
    "uate2e": "npm run e2e-tests -- --config baseUrl=https://uat.aegyris.ca ",
    "e2e-tests": "cypress run --browser chrome --headless --spec=cypress/e2e/tests/sequenced-tests.cy.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.+(js|ts|html|json)": [
      "eslint --fix"
    ]
  },
  "private": true,
